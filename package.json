{
    "name": "nodejs-express-api",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "NODE_ENV=development nodemon ./app/index.js --exec babel-node -e js",
        "build": "babel -d ./build ./app",
        "prebuild": "rm -rf build/*",
        "start": "NODE_ENV=production node ./build/index.js",
        "test": "NODE_ENV=test mocha --exit --require babel-core/register --require babel-polyfill ./app/test/",
        "testp": "NODE_ENV=test mocha --exit --require babel-core/register --require babel-polyfill --",
        "test-pipeline": "NODE_ENV=test mocha --exit --reporter mocha-junit-reporter --reporter-options mochaFile=./test-reports/junit.xml --require babel-core/register --require babel-polyfill ./app/test/"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.18.0",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.0.4",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.18.3",
        "express": "^4.16.3",
        "express-bearer-token": "^2.2.0",
        "express-validator": "^5.3.0",
        "faker": "^4.1.0",
        "jsonwebtoken": "^8.3.0",
        "moment": "^2.22.2",
        "nodemon": "^1.18.4",
        "pg": "^7.5.0",
        "pg-hstore": "^2.3.2",
        "sequelize": "^4.39.0",
        "sequelize-cli": "^4.1.1"
    },
    "devDependencies": {
        "babel-cli": "^6.26.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-0": "^6.24.1",
        "babel-register": "^6.26.0",
        "chai": "^4.2.0",
        "mocha": "^5.2.0",
        "mocha-junit-reporter": "^1.18.0",
        "supertest": "^3.3.0"
    }
}
